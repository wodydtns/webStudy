<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.buyer.dao.IBuyerOther">
	<select id="selectBuyerName" resultType="hashmap">
		SELECT buyer_name "buyer_name"
		FROM buyer
	</select>
	<select id="selectBuyerBank" resultType="hashmap">
		SELECT buyer_bank "buyer_bank"
		FROM buyer
	</select>
	
</mapper>